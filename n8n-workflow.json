    {
      "parameters": {
        "jsCode": "// Load external script for message extraction\nconst fs = require('fs');\nconst path = require('path');\n\n// Read the external script\nconst scriptPath = path.join(__dirname, '../../scripts/extract-message-data.js');\nconst scriptContent = fs.readFileSync(scriptPath, 'utf8');\n\n// Execute the script\nconst extractFunction = new Function('input', '$', scriptContent);\nreturn extractFunction($input.first().json, $);"
      },
      "id": "4f4e6b5a-1234-5678-abcd-extract12345",
      "name": "Extract Message Data",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [900, 240]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
